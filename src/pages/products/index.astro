---
import PageContent from "../[...lang]/products/index.astro";
import siteconfig from "@data/site.json";
import type { SupportedLanguage } from "@/utils/base";

// Get default language from config or fallback to 'en'
const defaultLang = siteconfig.defaultlang || 'en';

// Type assertion to ensure lang is a supported language
const lang = defaultLang as SupportedLanguage;

console.log("collection root Resolved language:", lang);
---

<PageContent {lang} />