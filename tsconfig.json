{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@data/*": ["src/data_files/*"],
      "@images/*": ["src/images/*"],
      "@scripts/*": ["src/assets/scripts/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@utils/*": ["src/utils/*"]
    },
    "types": [
      "astro/client",

      "vite/client",
      "vite-plugin-pwa/vanillajs",
      "vite-plugin-pwa/info",
      "vite-plugin-pwa/pwa-assets"
    ]
  }
}
